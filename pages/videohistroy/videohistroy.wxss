.box {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 20rpx;
}


.video-item {
    background-color: #fff;
    border-radius: 10rpx;
    margin-bottom: 20rpx;
}

.no{
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.more-txt {
    display: flex;
    flex-direction: row;
    color: #b6b6b6;
    font-size: 30rpx;
    height: 70rpx;
    line-height: 70rpx;
    justify-content: flex-end;
    align-items: flex-end;
    margin-top: -20rpx;
}

.listImg {
    width: 340rpx;
    height: 210rpx;
    border-radius: 10rpx;
}

.list-content {
    padding: 10rpx;
    display: flex;
    flex-direction: column;
    width: 320rpx;
    font-size: 28rpx;
    color: #c7c7c7;
}

.list-content-one {
    font-weight: bold;
    color: #000;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.list-content-txt-two {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.list-content-txt-two-info {
    width: 160rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}